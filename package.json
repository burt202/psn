{
  "name": "psn",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint --ext js,ts,tsx src/",
    "start": "nodemon --ext ts --watch src --exec ts-node ./src/index.ts",
    "develop": "./scripts/develop.sh"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/fit-commit-js/lib/hook.js .git/COMMIT_EDITMSG",
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.1",
    "@types/node": "^14.17.33",
    "@types/pg": "^8.6.1",
    "@types/ramda": "^0.27.59",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fit-commit-js": "^0.3.2",
    "husky": "^4.3.8",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "di-hard": "^2.0.0",
    "express": "^4.17.1",
    "jsonschema": "^1.4.0",
    "knex": "^0.95.14",
    "pg": "^8.7.1",
    "postgres-migrations": "^5.3.0",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  }
}
