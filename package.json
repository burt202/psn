{
  "name": "psn",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint --ext js,ts,tsx src/",
    "start-service": "nodemon --ext '*' --watch src/service --exec 'ts-node' src/service/index.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/fit-commit-js/lib/hook.js .git/COMMIT_EDITMSG",
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^14.17.33",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fit-commit-js": "^0.3.2",
    "husky": "^4.3.8",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
